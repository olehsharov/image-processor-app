{
  "name": "background-remover",
  "description": "Electron meets Vue.js and Parcel.js, the boilerplate",
  "version": "1.0.2",
  "author": {
    "name": "Oleg Sharov"
  },
  "repository": {
    "type": "git",
    "url": "https://https://github.com/olehsharov/image-processor-app.git"
  },
  "license": "MIT",
  "scripts": {
    "clean": "concurrently \"rm -fr build\" \"rm -fr .cache\"",
    "watch": "concurrently \"npm run watch:client\" \"npm run watch:server\"",
    "watch:client": "parcel watch client/index.html --out-dir build",
    "watch:server": "nodemon server/main.js --ignore client/ --ignore build/ --ignore test-data/",
    "build": "npm run clean && npm install && parcel build client/index.html --out-dir build",
    "dev_worker:import": "nodemon server/importWorker.js",
    "worker:import": "node server/importWorker.js",
    "worker": "concurrently \"npm run worker:import\" ",
    "server": "node server/main.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@fortawesome/vue-fontawesome": "^2.0.2",
    "@nodelib/fs.walk": "^1.2.6",
    "babel-polyfill": "^6.26.0",
    "bee-queue": "^1.3.1",
    "dotenv": "^8.2.0",
    "exif-js": "^2.3.0",
    "express": "^4.17.1",
    "forever": "^3.0.4",
    "global": "^4.4.0",
    "lodash": "^4.17.20",
    "md5": "^2.3.0",
    "object-hash": "^2.1.1",
    "parcel": "^1.12.4",
    "postcss-pseudo-classes": "^0.2.1",
    "puppeteer": "^5.5.0",
    "sharp": "^0.27.0",
    "tailwindcss": "^1.1.4",
    "tailwindcss-aspect-ratio": "^2.0.0",
    "uuid": "^8.3.2",
    "vue": "^2.6.11",
    "vue-hot-reload-api": "^2.3.4",
    "vue-img-orientation-changer": "^1.0.11",
    "vue-router": "^3.4.9",
    "vue-template-compiler": "^2.6.11"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-router": "^4.1.0",
    "@vue/cli-service": "^4.1.0",
    "@vue/component-compiler-utils": "^3.1.2",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "@vue/eslint-config-typescript": "^5.0.2",
    "babel-eslint": "^10.1.0",
    "concurrently": "^5.2.0",
    "eslint": "^6.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.0.0",
    "node-sass": "^4.12.0",
    "parcel": "^1.12.4",
    "parcel-bundler": "^1.12.4",
    "postcss-apply": "^0.12.0",
    "regenerator-runtime": "^0.13.3",
    "route-parser": "0.0.5",
    "sass": "^1.26.7",
    "sass-loader": "^8.0.0"
  }
}
